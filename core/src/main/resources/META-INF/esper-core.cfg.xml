<?xml version="1.0" encoding="UTF-8"?>
<esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.espertech.com/schema/esper"
    xsi:schemaLocation="http://www.espertech.com/schema/esper http://www.espertech.com/schema/esper/esper-configuration-4-0.xsd">

    <!-- Raw Events -->
    <event-type name="RawTick" class="ch.algotrader.vo.RawTickVO"/>
    <event-type name="RawBar" class="ch.algotrader.vo.RawBarVO"/>

    <!-- Tick Window -->
    <event-type name="SubscribeTickEvent" class="ch.algotrader.vo.SubscribeTickVO"/>
    <event-type name="UnsubscribeTickEvent" class="ch.algotrader.vo.UnsubscribeTickVO"/>

    <!-- Trade / Bid / Ask -->
    <event-type name="Trade" class="ch.algotrader.vo.TradeVO"/>
    <event-type name="Bid" class="ch.algotrader.vo.BidVO"/>
    <event-type name="Ask" class="ch.algotrader.vo.AskVO"/>

    <!-- Combinations / Components -->
    <event-type name="InsertComponentEvent" class="ch.algotrader.vo.InsertComponentEventVO"/>
    <event-type name="RemoveComponentEvent" class="ch.algotrader.vo.RemoveComponentEventVO"/>

    <!-- Performance -->
    <event-type name="MonthlyPerformance" class="ch.algotrader.vo.PeriodPerformanceVO"/>
    <event-type name="PerformanceKeys" class="ch.algotrader.vo.PerformanceKeysVO"/>
    <event-type name="MaxDrawDown" class="ch.algotrader.vo.MaxDrawDownVO"/>
    <event-type name="TradePerformance" class="ch.algotrader.vo.TradePerformanceVO"/>
    <event-type name="AllTrades" class="ch.algotrader.vo.TradesVO"/>
    <event-type name="WinningTrades" class="ch.algotrader.vo.TradesVO"/>
    <event-type name="LoosingTrades" class="ch.algotrader.vo.TradesVO"/>
    <event-type name="EndOfSimulation" class="ch.algotrader.vo.EndOfSimulationVO"/>

    <auto-import import-name="ch.algotrader.entity.trade.AlgoOrder"/>

    <!-- Constants -->
    <variable name="misc_marketIntrest" type="double" constant="true"/>
    <variable name="misc_defaultRetrievalSeconds" type="double" constant="true"/>

</esper-configuration>
