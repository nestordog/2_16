    <row>
        <field name="id">19</field>
        <field name="NAME">CREATE_INTERPOLATION</field>
        <field name="PRIORITY">0</field>
        <field name="DEFINITION">insert into Interpolation
select Math.exp(linest.YIntercept) as a,
linest.slope as b,
Math.pow(correl.correlation,2) as r
from Transaction.win:keepall().stat:linest(current_timestamp() / 31536000000, Math.log(account.portfolioValueDouble)) as linest,
Transaction.win:keepall().stat:correl(current_timestamp() / 31536000000, Math.log(account.portfolioValueDouble)) as correl</field>
