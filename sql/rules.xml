    <row>
        <field name="id">3</field>
        <field name="NAME">CREATE_PORTFOLIO_VALUE</field>
        <field name="PRIORITY">0</field>
        <field name="DEFINITION">insert into PortfolioValue
select current_timestamp as timestamp,
LookupUtil.getPortfolioValueAllAccounts() as value
from pattern[every timer:at (0, 9:17, *, *, 1:5)]</field>
