<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xmlns="http://www.espertech.com/schema/esper"    xsi:schemaLocation="http://www.espertech.com/schema/esper http://www.espertech.com/schema/esper/esper-configuration-3-0.xsd">    <event-type name="Tick" class="com.algoTrader.entity.TickImpl"/>    <event-type name="Transaction" class="com.algoTrader.entity.TransactionImpl"/>    <auto-import import-name="com.algoTrader.util.LookupUtil"/>    <auto-import import-name="com.algoTrader.stockOption.StockOptionUtil"/>    <auto-import import-name="com.algoTrader.util.DateUtil"/>    <auto-import import-name="com.algoTrader.entity.*"/>    <variable name="var_simulation" type="boolean" initialization-value="false"/>    <variable name="var_isin" type="string" initialization-value="CH00099808949"/>    <engine-settings>        <defaults>            <expression udf-cache="false"/>            <execution prioritized="true"/>            <!--            <threading>                <threadpool-inbound num-threads="2" enabled="true"/>                <threadpool-outbound num-threads="2" enabled="true"/>                <threadpool-timerexec num-threads="2" enabled="true"/>                <threadpool-routeexec num-threads="2" enabled="true"/>            </threading>            -->        </defaults>    </engine-settings></esper-configuration>
