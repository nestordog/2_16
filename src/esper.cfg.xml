<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xmlns="http://www.espertech.com/schema/esper"    xsi:schemaLocation="http://www.espertech.com/schema/esper http://www.espertech.com/schema/esper/esper-configuration-3-0.xsd">    <event-type name="Tick" class="com.algoTrader.entity.TickImpl"/>    <!-- <event-type name="Transaction" class="com.algoTrader.entity.TransactionImpl"/> -->    <event-type name="KFast" class="com.algoTrader.entity.KFastImpl"/>    <event-type name="KSlow" class="com.algoTrader.entity.KSlowImpl"/>    <event-type name="DSlow" class="com.algoTrader.entity.DSlowImpl"/>    <auto-import import-name="com.algoTrader.util.LookupUtil"/>    <auto-import import-name="com.algoTrader.stockOption.StockOptionUtil"/>    <auto-import import-name="com.algoTrader.util.DateUtil"/>    <auto-import import-name="com.algoTrader.entity.*"/>    <variable name="var_simulation" type="boolean" initialization-value="false"/>    <variable name="var_isin" type="string" initialization-value="CH00099808949"/>    <variable name="var_events_per_hour" type="int" initialization-value="60"/>    <engine-settings>        <defaults>            <expression udf-cache="false"/>            <execution prioritized="true"/>            <threading>                <internal-timer enabled="false" msec-resolution="100"/>            </threading>        </defaults>    </engine-settings></esper-configuration>
